{
    "name": "solcre/columnis-express",
    "description": "Content Management System that consumes Columnis API and assigns the data to a template engine.",
    "license": "BSD-3-Clause",
    "keywords": [
        "Solcre",
        "Columnis",
        "Express"
    ],
    "homepage": "http://www.columnis.com/",
    "repositories": [],
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": "2.3.3",
        "zendframework/zftool": "dev-master",
        "gkralik/gk-smarty": "dev-master",
        "zfcampus/zf-apigility": "~1.0",
        "guzzlehttp/guzzle": "~5.0",
        "guzzlehttp/cache-subscriber": "0.2.*@dev",
        "natxet/cssmin": "*",
        "mrclay/minify": "*",
        "solcre/zf2-conditional-loader": "dev-master",
	"zfr/zfr-cors": "1.*"        
    },
    "require-dev": {
        "zendframework/zendframework": "2.*",
        "phpunit/phpunit": "4.*",
        "squizlabs/php_codesniffer": "2.*",
        "zfcampus/zf-apigility-admin": "~1.0",
        "zfcampus/zf-development-mode": "~2.0"
