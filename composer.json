{
    "name": "solcre/columnis-express",
    "description": "Content Management System that consumes Columnis API and assigns the data to a template engine.",
    "license": "BSD-3-Clause",
    "keywords": [
        "Solcre",
        "Columnis",
        "Express"
    ],
    "homepage": "http://www.columnis.com/",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/solcre/gk-smarty"
        },
        {
            "type": "package",
            "package": {
                "name": "cssmin/cssmin",
                "version": "3.0.1",
                "dist": { "url": "http://cssmin.googlecode.com/files/cssmin-v3.0.1.php", "type": "file" },
                "autoload": { "classmap": [ "cssmin-v3.0.1.php" ] }
            }
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": "2.3.3",
        "zendframework/zftool": "dev-master",
        "gkralik/gk-smarty": "dev-master",
        "rwoverdijk/assetmanager": "dev-master",
        "guzzle/guzzle": "dev-master",
        "guzzle/plugin-cache" : "dev-master",
        "cssmin/cssmin": "*",
        "mrclay/minify": "*",
        "solcre/zf2-conditional-loader": "dev-master"        
    },
    "require-dev": {
        "zendframework/zendframework": "2.*",
        "phpunit/phpunit": "4.*",
        "squizlabs/php_codesniffer": "2.*"
    },
    "autoload": {
        "psr-0": {
            "ColumnisExpress": "src/"
        }
    }
}
